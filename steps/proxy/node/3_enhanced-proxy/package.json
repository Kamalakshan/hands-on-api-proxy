{
  "name": "proxy-node-server",
  "version": "1.0.0",
  "description": "Node server which proxies requests for mobile applications",
  "author": "Skip Hovsmith",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "prestart": "babel-node scripts/start-msg.js",
    "all:start": "npm-run-all --parallel start sec:check test:watch",
    "start": "babel-node src/index.js",
    "sec:check": "nsp check",
    "test": "mocha --reporter progress --compilers js:babel-register \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "build": "babel src -d dist --ignore \"**/*.test.js\"",
    "clean": "rimraf ./dist && mkdir dist",
    "serve": "node dist/index.js"
  },
  "main": "src/index.js",
  "dependencies": {
    "chalk": "^1.1.3",
    "cross-env": "^4.0.0",
    "express": "^4.15.2",
    "jsonwebtoken": "^7.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "rimraf": "^2.6.1"
  }
}
